<h2>Show user</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @user.name %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

  <li>
    <strong>Caren ID:</strong>
    <%= @user.caren_id %>
  </li>

  <li>
    <strong>Fitbit ID:</strong>
    <%= @user.fitbit_id %>
  </li>


</ul>

<span><%= link "Edit", to: user_path(@conn, :edit, @user) %></span>
<span><%= link "Back", to: user_path(@conn, :index) %></span>
